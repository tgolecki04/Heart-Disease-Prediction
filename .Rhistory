p3 <- ggplot(badania, aes(x = diaBP)) + geom_density() + theme_ipsum() + ggtitle("diaBP")
p3 <- ggplot(badania, aes(x = heartRate)) + geom_density() + theme_ipsum() + ggtitle("diaBP")
<- (p1 | p2 | p3)
